{
  "name": "strider-gitlab",
  "version": "0.0.0",
  "description": "A gitlab provider for strider",
  "main": "index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodefourtytwo/strider-gitlab-provider.git"
  },
  "keywords": [
    "git",
    "gitlab",
    "strider"
  ],
  "author": "Joel Dimbernat",
  "license": "MIT",
  "readmeFilename": "Readme.md",
  "strider": {
    "id": "gitlab",
    "title": "GitLab",
    "type": "provider",
    "hosted": true,
    "config": true,
    "accountConfig": {
      "template": "config/accountConfig.html"
    },
    "webapp": "webapp.js",
    "worker": "worker.js",
    "inline_icon": "bitbucket"
  },
  "devDependencies": {
    "jshint": "~2.1.11",
    "expect.js": "~0.2.0",
    "mocha": "~1.13.0"
  },
  "dependencies": {
    "superagent": "~0.15.4",
    "strider-git": "git+https://github.com/Strider-CD/strider-git.git",
    "request": "~2.27.0",
    "lodash": "~2.2.0",
    "step": "0.0.5",
    "async": "~0.2.9",
    "crypto": "0.0.3"
  }
}
